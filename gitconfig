[alias]
  n = "!n() { \
    local last_branch; \
    last_branch=$(git symbolic-ref --short HEAD); \
    git stash apply stash; \
    git checkout master; \
    git fetch; \
    git merge origin/master; \
    git checkout ${last_branch}; \
    git merge origin/master; \
  }; n" 
  
  a = "add"
  c = "commit"
  co = "checkout"
  l = "log"
  m = "merge"
  p = "push"
  s = "status"
